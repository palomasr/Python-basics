    "import numpy as np\n",
    "from numpy.random import randn\n",
    "\n",
    "N = 7000\n",
    "counter = 0\n",
    "for i in randn(N):\n",
    "    if (i > -1 and i < 1):\n",
    "        counter = counter + 1\n",
    "answer = counter / N\n",
    "print (answer)"
   ]
